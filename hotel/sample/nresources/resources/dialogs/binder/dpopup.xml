<?xml version="1.0"?>
<ui:UiBinder xmlns:ui='http://www.jythongwt.uibinderxsd.com'
        xmlns:g='http://www.jythongwt.uibinderxsd.com' xmlns:p='http://www.jythongwt.uibinderxsd.com'
        xmlns:i='http://www.jythongwt.uibinderxsd.com' xmlns='http://www.jythongwt.uihtml.com'>

<p:PaperDialog modal="true">
    
    <paper-dialog-scrollable>

    <p:PaperInput label="String copied from parent" readonly="true" fieldid="dstring" />

    <p:PaperInput label="String to copy to parent" fieldid="newstring" />

    </paper-dialog-scrollable>
    
    <div class="buttons">
        <p:PaperButton attributes="dialog-dismiss" fieldid="ok">My Button</p:PaperButton>
     </div>
</p:PaperDialog>

    
</ui:UiBinder>