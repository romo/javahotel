<?xml version="1.0"?>
<ui:UiBinder xmlns:ui='http://www.jythongwt.uibinderxsd.com'
        xmlns:g='http://www.jythongwt.uibinderxsd.com' xmlns:p='http://www.jythongwt.uibinderxsd.com'
        xmlns:i='http://www.jythongwt.uibinderxsd.com' xmlns='http://www.jythongwt.uihtml.com'>

<g:HTMLPanel>


<p:PaperDialog fieldid="dialog" modal="true">
    <h2><g:Label fieldid="header"> </g:Label></h2>
    <paper-dialog-scrollable>
      <div id="content">XXXXX</div>
    </paper-dialog-scrollable>
  <div class="buttons">
      <p:PaperButton fieldid="yes"><i:IronIcon icon="check"/>  {@yes}</p:PaperButton>
      <p:PaperButton fieldid="no"><i:IronIcon icon="clear"/>  {@no}</p:PaperButton>
  </div>
</p:PaperDialog>

</g:HTMLPanel>
    
</ui:UiBinder>