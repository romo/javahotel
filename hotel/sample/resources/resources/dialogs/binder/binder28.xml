<?xml version="1.0"?>
<ui:UiBinder xmlns:ui='http://www.jythongwt.uibinderxsd.com'
	xmlns:g='http://www.jythongwt.uibinderxsd.com' xmlns:p='http://www.jythongwt.uibinderxsd.com'
	xmlns:i='http://www.jythongwt.uibinderxsd.com' xmlns='http://www.jythongwt.uihtml.com'>

	<ui:style>
		.sized {
		width: 200px;
		height: 200px;
		}

		.gray {
		background-color: lightgray;
		}

		.group {
		display: inline-block;
		vertical-align: top;
		}

		.controls {
		display: block;
		margin-bottom: 1em;
		}
	</ui:style>

	<g:HTMLPanel>
		<h3>Sizing: none (naturally sized)</h3>
		<i:IronImage src="http://vaadin.github.io/gwt-polymer-elements/demo/gwtPolymerDemo/polymer.svg" />

		<h3>Sizing: cover</h3>
		<i:IronImage addStyleNames="{style.sized}" sizing="cover"
			src="http://vaadin.github.io/gwt-polymer-elements/demo/gwtPolymerDemo/polymer.svg" />
		<i:IronImage addStyleNames="{style.sized}" sizing="cover"
			src="./polymer.svg" />

		<h3>Sizing: contain</h3>
		<i:IronImage addStyleNames="{style.sized} {style.gray}"
			sizing="contain" src="http://vaadin.github.io/gwt-polymer-elements/demo/gwtPolymerDemo/polymer.svg" />
		<i:IronImage addStyleNames="{style.sized} {style.gray}"
			sizing="contain" src="http://vaadin.github.io/gwt-polymer-elements/demo/gwtPolymerDemo/polymer.svg" />

		<h3>Preload: none</h3>
		<div class="{style.group}">
			<div>No sizing</div>
			<div class="{style.controls}">
				<g:Button fieldid="button1a">Load image</g:Button>
			</div>
			<i:IronImage fieldid="image1a" addStyleNames="{style.sized} {style.gray}" />
		</div>
		<div class="{style.group}">
			<div>Cover</div>
			<div class="{style.controls}">
				<g:Button fieldid="button1b">Load image</g:Button>
			</div>
			<i:IronImage fieldid="image1b" addStyleNames="{style.sized} {style.gray}"
				sizing="cover" />
		</div>
		<div class="{style.group}">
			<div>Contain</div>
			<div class="{style.controls}">
				<g:Button fieldid="button1c">Load image</g:Button>
			</div>
			<i:IronImage fieldid="image1c" addStyleNames="{style.sized} {style.gray}"
				sizing="contain" />
		</div>


		<h3>Preload: image as placeholder</h3>
		<div class="{style.group}">
			<div>No sizing</div>
			<div class="{style.controls}">
				<g:Button fieldid="button3a">Load image</g:Button>
			</div>
			<i:IronImage fieldid="image3a" addStyleNames="{style.sized} {style.gray}"
				preload="true"
				placeholder="data:image/gif;base64," />
		</div>

		<div class="{style.group}">
			<div>Cover</div>
			<div class="{style.controls}">
				<g:Button fieldid="button3b">Load image</g:Button>
			</div>
			<i:IronImage fieldid="image3b" addStyleNames="{style.sized} {style.gray}"
				sizing="cover" preload="true"
				placeholder="data:image/gif;base64," />
		</div>

		<div class="{style.group}">
			<div>Contain</div>
			<div class="{style.controls}">
				<g:Button fieldid="button3c">Load image</g:Button>
			</div>
			<i:IronImage fieldid="image3c" addStyleNames="{style.sized} {style.gray}"
				sizing="contain" preload="true"
				placeholder="data:image/gif;base64," />
		</div>


		<h3>Preload: image as placeholder, with Fade-in</h3>
		<div class="{style.group}">
			<div>No sizing</div>
			<div class="{style.controls}">
				<g:Button fieldid="button3aFade">Load image</g:Button>
			</div>
			<i:IronImage fieldid="image3aFade" addStyleNames="{style.sized} {style.gray}"
				preload="true" fade="true"
				placeholder="data:image/gif;base64," />
		</div>

		<div class="{style.group}">
			<div>Cover</div>
			<div class="{style.controls}">
				<g:Button fieldid="button3bFade">Load image</g:Button>
			</div>
			<i:IronImage fieldid="image3bFade" addStyleNames="{style.sized} {style.gray}"
				sizing="cover" preload="true" fade="true"
				placeholder="data:image/gif;base64," />
		</div>

		<div class="{style.group}">
			<div>Contain</div>
			<div class="{style.controls}">
				<g:Button fieldid="button3cFade">Load image</g:Button>
			</div>
			<i:IronImage fieldid="image3cFade" addStyleNames="{style.sized} {style.gray}"
				sizing="contain" preload="true" fade="true"
				placeholder="data:image/gif;base64," />
		</div>
	</g:HTMLPanel>

</ui:UiBinder>