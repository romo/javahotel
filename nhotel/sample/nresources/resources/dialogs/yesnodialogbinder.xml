<?xml version="1.0" encoding="UTF-8"?>
<ui:UiBinder xmlns:ui='http://www.jythongwt.uibinderxsd.com'        xmlns:g='http://www.jythongwt.uibinderxsd.com' xmlns:p='http://www.jythongwt.uibinderxsd.com'        xmlns:i='http://www.jythongwt.uibinderxsd.com' xmlns='http://www.jythongwt.uihtml.com'>
  <p:PaperDialog modal="true">
    <h2>
      <g:Label fieldid="header">
      </g:Label>
    </h2>
    <paper-dialog-scrollable>
      <div id="content">XXXXX</div>
    </paper-dialog-scrollable>
    <div class="buttons">
      <p:PaperButton fieldid="yes" attributes="dialog-confirm">
        <i:IronIcon icon="check"/>  {@yes}</p:PaperButton>
        <p:PaperButton fieldid="no" attributes="dialog-dismiss">
          <i:IronIcon icon="clear"/>  {@no}</p:PaperButton>
        </div>
      </p:PaperDialog>
    </ui:UiBinder>
